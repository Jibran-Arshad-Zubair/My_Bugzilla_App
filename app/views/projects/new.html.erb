<h1>Create New Project</h1>

<%= form_for @project do |f| %>
  <div class="field">
    <%= f.label :name %>
  <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <%= f.submit 'Create Project' %>
<% end %>

<%= link_to 'Back to Projects', projects_path %>
